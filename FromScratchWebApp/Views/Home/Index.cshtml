
@{
    //ViewData["Title"] = "Index";
    ViewBag.Title = "Index";
}

<h1>Index of ASP.NET</h1>

<p>We are using Razor to</p>

<table>

    @for (int y = 1; y < 11; y++)
    {
    <tr>
        @for (int x = 1; x < 11; x++)
        {
            <td>@( y * x )</td>
        }
    </tr>
    }
</table>